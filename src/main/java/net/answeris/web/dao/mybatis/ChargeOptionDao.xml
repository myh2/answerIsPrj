<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.answeris.web.dao.mybatis.ChargeOptionDao">
	<select id="get" resultType="net.answeris.web.model.ChargeOption">
		select * from ChargeOption where Code=#{code}
	</select>

	<select id='getList' resultType="net.answeris.web.model.ChargeOption">
		select * from ChargeOption where AdminCode=#{adminCode}
	</select>
</mapper>